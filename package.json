{
  "name": "mwavuli",
  "version": "1.0.0",
  "description": "Mwavuli is an open source project which enables developers to add pesa(money) functionality to an application that lacks this. Feel free to fork contribute",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node ./bin/www",
    "test": "mocha",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sylvance/mwavuli.git"
  },
  "keywords": [
    "mpesa",
    "nodejs",
    "cart",
    "money",
    "api"
  ],
  "author": "Sylvance Kerandi",
  "contributors": [
    "Sylvance Kerandi"
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Sylvance/mwavuli/issues"
  },
  "homepage": "https://github.com/Sylvance/mwavuli#readme",
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "express": "~4.13.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.3",
    "mocha-lcov-reporter": "^1.3.0"
  }
}
